<?xml version='1.0'?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-waj">
    <comment>Waj archive files</comment>
    <magic priority="50">
      <match type="string" offset="0:256" value="jbk_waj" mask="0xffffff00ffffff"/>
    </magic>
    <glob pattern="*.waj"/>

    <icon name="package-x-generic" />
    <generic-icon name="package-x-generic" />
  </mime-type>
</mime-info>
